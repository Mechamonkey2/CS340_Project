<form>
    <h3 class="heading">Data</h3>
        <table class="data">
			<tr>
				<td> Item ID </td>
				<td> Consumable ID </td>
				<td> Gear ID </td>
			</tr>
			{{#each Page}}
				<tr>
					<td> {{itemID}} </td>
					<td> {{consumableID}} </td>
					<td> {{gearID}} </td>
				</tr>
			{{/each}}
        </table>
</form>

<h2 class="heading">Edit Data</h2>
<form>
	<table class="data">
		<tr>
			<td> Item ID<input type ="text" id="itemID" name="itemID"> </td>
		</tr>
		<tr>
			<td> Consumable ID <input type ="text" id="consumableID" name="consumableID"> </td>
		</tr>
		<tr>
			<td> Gear ID <input type ="text" id="gearID" name="gearID"> </td>
		</tr>
		<tr>
			<th>  <input type="submit" value="Edit"> </th>
		</tr>
    </table>
</form>

<h2 class="heading"> Remove Data </h2>
<form>
    <table class="data">
        <tr>
			<td>
				Item ID<input type ="text" id="itemID" name="itemID">
			</td>
        </tr>
        <tr>
			<th>  <input type="submit" value="Remove"> </th>
        </tr>
	</table>
</form>
<h2 class="heading"> Search Data </h2>
<form action="/Item_IDs" method="POST">
    <table class="data">
        <tr>
			<td>
				<select name="searchData" id="searchData">
					<option value="itemID">Item ID</option>
					<option value="consumableID">Consumable ID</option>
					<option value="gearID">Gear ID</option>
				</select> <input type ="text" id="searchDataAmount" name="searchDataAmount">
			</td>
        </tr>
        <tr>
			<th>  <input type="submit" value="Search"> </th>
        </tr>
	</table>
</form>
